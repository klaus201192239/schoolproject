<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF" >

    <RelativeLayout
        android:id="@+id/activityinte_title"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/interalac" >

        <Button
            android:id="@+id/activityinte_back"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="5dp"
            android:background="@drawable/wm_back2"
            android:onClick="to_back" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/activityinte_listRL"
        android:layout_width="90dp"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/activityinte_title"
        android:background="#EFEFEF" >

        <ListView
            android:id="@+id/activityinte_list1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:divider="#00000000"
            android:gravity="center" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/activityinte_title"
        android:layout_toRightOf="@id/activityinte_listRL"
        android:background="#FFFFFF" >

        <Spinner
            android:id="@+id/activityinte_year"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="5dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/integraldown"
            android:paddingLeft="5dp"
            android:textColor="#a9a9a9"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/activityinte_year"
            android:layout_marginTop="15dp"
            android:gravity="center"
            android:textColor="#FFCC33"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/integal_sum"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/text_title"
            android:layout_marginRight="5dp"
            android:layout_marginTop="10dp"
            android:gravity="right" />

        <RelativeLayout
            android:id="@+id/activityinte_temp"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_below="@id/integal_sum"
            android:layout_marginTop="5dp"
            android:background="#EFEFEF" >
            
        </RelativeLayout>

        <ListView
            android:id="@+id/activityinte_list2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/activityinte_temp"
            android:divider="#EFEFEF"
            android:dividerHeight="5dp"
            android:scrollbars="none" />
    </RelativeLayout>

</RelativeLayout>